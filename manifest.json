{
  "manifest_version": 2,
  "name": "Cloudlfare dings",
  "version": "1.0",
  "description": "Adds a red border to all webpages matching mozilla.org.",
  "icons": {
    "48": "cloudflare-icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://dash.cloudflare.com/*"],
      "js": ["cloudflare_dings.js"]
    }
  ],
  "permissions": ["storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "tim@witzdam.com"
    }
  }
}
